{
  "name": "trixly",
  "version": "0.0.3",
  "description": "TypeScript starter kit",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf coverage/ dist/",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development",
    "build:prod": "cross-env NODE_ENV=production",
    "test": "npm run test:browser",
    "test:browser": "cross-env NODE_ENV=test karma start config/karma.conf.js --no-auto-watch",
    "test:phantom": "cross-env NODE_ENV=test karma start config/karma.conf.js --single-run --no-auto-watch --browsers=PhantomJS",
    "test:chrome": "cross-env NODE_ENV=test karma start config/karma.conf.js --single-run --no-auto-watch --browsers=Chrome",
    "lint": "npm run lint:src && npm run lint:tests",
    "lint:tests": "tslint test/**/*.ts",
    "lint:src": "tslint src/**/*.ts",
    "postinstall": "typings install",
    "dependencies:check": "ncu",
    "dependencies:upgrade": "ncu -u"
  
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trixler/trixly.git"
  },
  "author": "...",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trixler/trixly/issues"
  },
  "keywords": [
    "vscode",
    "typescript",
    "boilerplate",
    "project",
    "skeleton",
    "debug",
    "intellisense",
    "compile",
    "transpile",
    "mocha"
  ],
  "homepage": "https://github.com/trixler/trixly#readme",
  "dependencies": {
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "karma-expect": "1.1.2",
    "karma-mocha": "1.0.1",
    "karma-chai": "0.1.0",
    "karma-sinon": "1.0.5",
    "run-sequence": "^1.2.1",
    "semver": "^5.1.0",
    "expect": "1.20.1",
    "phantomjs-prebuilt": "^2.1.7",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "codelyzer": "~0.0.22",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4",
    "chai": "^3.5.0",
    "typescript": "^1.9.0-dev.20160607-1.0",
    "typings": "^1.0.4",
    "source-map-support": "^0.4.0",
    "source-map-loader": "^0.1.5",
    "tslint": "^3.10.0-dev.3",
    "ts-node": "^0.8.0",
    "typedoc": "^0.4.1",
    "walk": "^2.3.9",
    "yargs": "^4.7.1",
    "awesome-typescript-loader": "~0.19.1",
    "tslint-loader": "^2.1.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0",
    "npm-check-updates": "^2.6.1",
    "cross-env": "1.0.8"
  },
  "engines": {
    "node": ">= 5.4.1 < 7"
  }
}
